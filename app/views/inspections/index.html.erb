<h1>Listing inspections</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Inspector</th>
      <th>Eq number</th>
      <th>Protocol number</th>
      <th>Facility</th>
      <th>Protocol file</th>
      <th>Inspector company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inspections.each do |inspection| %>
      <tr>
        <td><%= inspection.date %></td>
        <td><%= inspection.inspector %></td>
        <td><%= inspection.eq_number %></td>
        <td><%= inspection.protocol_number %></td>
        <td><%= inspection.facility %></td>
        <td><%= inspection.protocol_file %></td>
        <td><%= inspection.inspector_company %></td>
        <td><%= link_to 'Show', inspection %></td>
        <td><%= link_to 'Edit', edit_inspection_path(inspection) %></td>
        <td><%= link_to 'Destroy', inspection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inspection', new_inspection_path %>
