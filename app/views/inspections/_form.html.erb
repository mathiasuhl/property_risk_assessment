<%= simple_form_for(@inspection) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :inspector %>
    <%= f.input :eq_number %>
    <%= f.input :protocol_number %>
    <%= f.association :facility %>
    <%= f.input :protocol_file %>
    <%= f.input :inspector_company %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
