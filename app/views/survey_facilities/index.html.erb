<h1>Listing survey_facilities</h1>

<table>
  <thead>
    <tr>
      <th>Survey</th>
      <th>Facility</th>
      <th>Has grap packs</th>
      <th>Has service stickers</th>
      <th>Elevator emergencey call is working</th>
      <th>Elevator intercom is working</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_facilities.each do |survey_facility| %>
      <tr>
        <td><%= survey_facility.survey_id %></td>
        <td><%= survey_facility.facility_id %></td>
        <td><%= survey_facility.has_grap_packs %></td>
        <td><%= survey_facility.has_service_stickers %></td>
        <td><%= survey_facility.elevator_emergencey_call_is_working %></td>
        <td><%= survey_facility.elevator_intercom_is_working %></td>
        <td><%= link_to 'Show', survey_facility %></td>
        <td><%= link_to 'Edit', edit_survey_facility_path(survey_facility) %></td>
        <td><%= link_to 'Destroy', survey_facility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey facility', new_survey_facility_path %>
